{
  "name": "@lionweb/repository-packages",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/LionWeb-io/lionweb-repository.git"
  },
  "bugs": {
    "url": "https://github.com/LionWeb-io/lionweb-repository/issues"
  },
  "scripts": {
