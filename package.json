  },
  "devDependencies": {
    "prettier": "3.1.0",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "@types/chai": "4.3.11",
    "@types/mocha": "10.0.1",
    "eslint": "8.56.0",
    "chai": "4.3.10",
    "mocha": "10.2.0",
    "ts-node": "10.9.2",
    "typescript": "5.2.2"
  },
  "workspaces": [
    "./packages/dbadmin",
    "./packages/inspection",
    "./packages/core",
    "./packages/test"
